{
  "name": "functional-ts",
  "type": "module",
  "version": "0.0.0",
  "description": "Project for exploring functional libraries in TypeScript",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/functional-ts.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "build": "bun run clean && bun test && tsc",
    "clean": "del-cli lib coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "bun run build",
    "prepare": "husky install",
    "release": "release-it --ci",
    "test": "bun test",
    "test:cov": "bun test --coverage",
    "test:watch": "bun test --watch",
    "watch": "tsc -c"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/bun": "1.1.3",
    "@types/node": "20.12.12",
    "@types/sanctuary": "3.1.4",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "del-cli": "5.1.0",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.5",
    "prettier": "3.2.5",
    "release-it": "17.3.0",
    "testdouble": "3.20.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@effect/platform": "0.55.0",
    "@effect/schema": "0.67.13",
    "@mobily/ts-belt": "4.0.0-rc.5",
    "@rimbu/core": "2.0.2",
    "@swan-io/boxed": "2.3.0",
    "@unsplash/sum-types": "0.4.1",
    "collectable": "0.31.1",
    "effect": "3.2.5",
    "fluture": "14.0.0",
    "fp-ts": "2.16.6",
    "immutable": "5.0.0-beta.5",
    "list": "2.0.19",
    "monet": "0.9.3",
    "neverthrow": "6.2.1",
    "oxide.ts": "1.1.0",
    "pify": "6.1.0",
    "pratica": "2.2.0",
    "prelude-ts": "1.0.6",
    "purifree-ts": "1.2.3",
    "purify-ts": "2.0.3",
    "remeda": "1.61.0",
    "rxjs": "7.8.1",
    "sanctuary": "3.1.0",
    "shades": "2.2.0",
    "true-myth": "7.3.0",
    "ts-option": "2.1.0",
    "ts-pattern": "5.1.2",
    "wonka": "6.3.4"
  }
}
