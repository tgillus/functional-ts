{
  "name": "functional-ts",
  "type": "module",
  "version": "0.0.0",
  "description": "Project for exploring functional libraries in TypeScript",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/functional-ts.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky install",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/node": "20.10.5",
    "@types/sanctuary": "3.1.4",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "@vitest/coverage-v8": "1.1.0",
    "del-cli": "5.1.0",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "release-it": "17.0.1",
    "testdouble": "3.20.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vitest": "1.1.0"
  },
  "dependencies": {
    "@mobily/ts-belt": "4.0.0-rc.5",
    "@rimbu/core": "1.1.1",
    "@swan-io/boxed": "2.0.0",
    "@unsplash/sum-types": "0.4.1",
    "collectable": "0.31.1",
    "effect": "2.0.0-next.62",
    "fluture": "14.0.0",
    "fp-ts": "2.16.1",
    "immutable": "5.0.0-beta.4",
    "list": "2.0.19",
    "monet": "0.9.3",
    "neverthrow": "6.1.0",
    "oxide.ts": "1.1.0",
    "pify": "6.1.0",
    "pratica": "2.2.0",
    "prelude-ts": "1.0.6",
    "purifree-ts": "1.2.3",
    "purify-ts": "2.0.1",
    "remeda": "1.33.0",
    "rxjs": "7.8.1",
    "sanctuary": "3.1.0",
    "shades": "2.2.0",
    "true-myth": "7.1.0",
    "ts-option": "2.1.0",
    "ts-pattern": "5.0.6",
    "wonka": "6.3.4"
  }
}
