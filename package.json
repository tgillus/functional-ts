{
  "name": "functional-ts",
  "type": "module",
  "version": "0.0.0",
  "description": "Project for exploring functional libraries in TypeScript",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/functional-ts.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del ./lib ./coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky install",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@release-it/conventional-changelog": "5.1.1",
    "@types/node": "18.11.18",
    "@types/sanctuary": "3.1.0",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitest/coverage-c8": "0.26.3",
    "del-cli": "5.0.0",
    "eslint": "8.31.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "release-it": "15.6.0",
    "testdouble": "3.16.8",
    "tsx": "3.12.1",
    "typescript": "4.9.4",
    "vitest": "0.26.3"
  },
  "dependencies": {
    "@mobily/ts-belt": "4.0.0-rc.3",
    "@rimbu/core": "0.11.0",
    "@unsplash/sum-types": "0.3.1",
    "collectable": "0.31.1",
    "fluture": "14.0.0",
    "fp-ts": "2.13.1",
    "hkt-ts": "2.1.5",
    "immutable": "4.2.1",
    "list": "2.0.19",
    "monet": "0.9.3",
    "neverthrow": "6.0.0",
    "oxide.ts": "1.1.0",
    "pify": "6.1.0",
    "pratica": "2.1.0",
    "prelude-ts": "1.0.5",
    "purifree-ts": "1.2.3",
    "purify-ts": "1.3.0",
    "remeda": "1.3.0",
    "rxjs": "7.8.0",
    "sanctuary": "3.1.0",
    "shades": "2.2.0",
    "true-myth": "6.2.0",
    "ts-option": "2.1.0",
    "ts-pattern": "4.0.6",
    "wonka": "6.1.2"
  }
}
